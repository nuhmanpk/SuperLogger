{
  "name": "superlogger",
  "displayName": "Super Logger",
  "description": "A VS Code extension that enhances debugging in various programming languages by adding fun and informative print statements with random emojis and jokes",
  "publisher": "nuhmanpk",
  "icon": "images/logo.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Debuggers",
    "Testing",
    "Programming Languages",
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nuhmanpk/SuperLogger.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "superlogger.addSuperLogger",
        "title": "Add SuperLogger"
      },
      {
        "command": "superlogger.nodeLogger",
        "title": "Comment all Logs"
      }
    ],
    "keybindings": [
      {
        "command": "superlogger.addSuperLogger",
        "key": "ctrl+alt+p",
        "mac": "cmd+alt+p",
        "when": "editorTextFocus",
        "title": "Add Super Log"
      },
      {
        "command": "superlogger.nodeLogger",
        "key": "ctrl+alt+k",
        "mac": "cmd+alt+k",
        "when": "editorTextFocus",
        "title": "For Node"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
